#!/bin/bash
############################################################################################################################
#
# Outsourcer start all script
#
############################################################################################################################
PWD=$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )
source $PWD/../os_path.sh

echo "############################################################################################################################"
echo "#"
echo "# Start gpfdist"
echo "#"
echo "############################################################################################################################"
gpfdiststart

echo "############################################################################################################################"
echo "#"
echo "# Start UI"
echo "#"
echo "############################################################################################################################"
uistart

echo "############################################################################################################################"
echo "#"
echo "# Start Queue Daemon"
echo "#"
echo "############################################################################################################################"
osstart

echo "############################################################################################################################"
echo "#"
echo "# Start Scheduler Daemon"
echo "#"
echo "############################################################################################################################"
agentstart

echo "############################################################################################################################"
echo "#"
echo "# Start gpfdist for Custom tables"
echo "#"
echo "############################################################################################################################"
customstartall
