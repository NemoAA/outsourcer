#!/bin/bash
############################################################################################################################
#
# Outsourcer stop all script
#
############################################################################################################################
set -e
PWD=$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )
source $PWD/../os_path.sh

echo "############################################################################################################################"
echo "#"
echo "# Stop Scheduler Daemon"
echo "#"
echo "############################################################################################################################"
agentstop

echo "############################################################################################################################"
#
echo "#"
echo "# Stop Queue Daemon"
echo "#"
echo "############################################################################################################################"
osstop

echo "############################################################################################################################"
echo "#"
echo "# Stop gpfdist"
echo "#"
echo "############################################################################################################################"
gpfdiststop

echo "############################################################################################################################"
echo "#"
echo "# Stop UI"
echo "#"
echo "############################################################################################################################"
uistop

